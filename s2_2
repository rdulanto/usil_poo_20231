    //Switch

    String fecha = "30/03/2023";
    String mes = fecha.substring(3, 5);
    int m = Integer.parseInt(mes);
    String mesN;
    switch (m){
      case 1: 
        mesN = "Enero";
        break;
      case 2:
        mesN = "Febrero";
        break;
      case 3:
        mesN = "Marzo";
        break;
      case 4:
        mesN = "Abril";
        break;
      case 5:
        mesN = "Mayo";
        break;
      case 6:
        mesN = "Junio";
        break;
      case 7:
        mesN = "Julio";
        break;
      case 8:
        mesN = "Agosto";
        break;
      case 9:
        mesN = "Setiembre";
        break;
      case 10:
        mesN = "Obtubre";
        break;
      case 11:
        mesN = "Noviembre";
        break;
      case 12:
        mesN = "Diciembre";
        break;
      default:
        mesN = "No existe";
        break;
    }

    System.out.println("El mes es: " + mesN);
  }
}
